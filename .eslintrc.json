{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "globals": { "fetch": false },
  "rules": {
    "semi": ["error", "never"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": ["error", { "ignore": ["tintColor", "navigation"] }],
    "no-nested-ternary": 0,
    "no-use-before-define": ["error", { "variables": false }]
  }
}